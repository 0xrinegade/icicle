{
    "name": "Icicle Examples: rust poseidon hash",
    "build": {
        "dockerfile": "Dockerfile"
    },
    "runArgs": [
        "--gpus",
        "all"
    ],
    "postCreateCommand": [
        "nvidia-smi"
	],
	"customizations": {
